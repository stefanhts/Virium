		}
public void close(boolean check){
	CreatePopUp window = new CreatePopUp("ERROR");
	if(check==true){
	window.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);}
	else{window.setDefaultCloseOperation(JFrame.DO_NOTHING_ON_CLOSE);}
}
	}


